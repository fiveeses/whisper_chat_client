{
	"source": "./src",
	"destination": "./docs",
	"plugins": [
      {
         "name": "esdoc-standard-plugin",
         "option": {
            "test": {
               "source": "./src",
               "interfaces": ["describe", "it"],
               "includes": ["spec\\.js$"],
               "excludes": []
            }
         }
      }
	]
}